
  <div id="wrapper">
    <div id="im">
      <h1>Welcome to RecMe, your spot where you sport!</h1>

      <div id="user_header">
        <% if current_user %>
          Logged in as
          <%= current_user.name %>
          <%= link_to "Log Out", logout_path %>
          <%= link_to 'New Post', new_post_path %>
        <% else %>
          <%= link_to "Sign Up", signup_path %>
          or
          <%= link_to "Log In", login_path %>
        <% end %>
      </div>

    </div>
  </div>
